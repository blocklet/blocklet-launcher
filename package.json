{
  "name": "install-on-abtnode",
  "title": "Install On ABT Node",
  "version": "1.1.4",
  "description": "A simple blocklet that helps you install blocklet on abtnode",
  "keywords": [
    "dapp",
    "forge"
  ],
  "files": [
    "build",
    "screenshots",
    "logo.png",
    "blocklet.md",
    "blocklet.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blocklet/install-on-abtnode"
  },
  "dependencies": {
    "@abtnode/client": "^1.0.21",
    "@arcblock/ux": "^0.78.15",
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "flat": "^5.0.2",
    "http-proxy-middleware": "^0.19.1",
    "is-url": "^1.2.4",
    "material-table": "^1.69.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dev-utils": "^11.0.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.0",
    "react-use": "^13.26.3",
    "serverless-http": "^2.0.2",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "lint": "eslint src api",
    "lint:fix": "eslint --fix src api",
    "precommit": "yarn lint",
    "prepush": "yarn lint",
    "clean": "rm -rf build api/dist .blocklet",
    "start": "npm-run-all --parallel start:*",
    "start:client": "PUBLIC_URL=\"\" react-app-rewired start",
    "build": "yarn clean && npm run build:client && rm -f build/static/**/*.{js,css}.map",
    "build:client": "PUBLIC_URL=\"/z8iZmTHdfidsHjmzCLeSUbdj4yrABe1CewFvF\" react-app-rewired build",
    "release": "npm run clean && npm run build",
    "deploy": "npm run release && abtnode deploy .blocklet/bundle --skip-hooks"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "mutationobserver-shim": "^0.3.7",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.6"
  },
  "license": "Apache-2.0",
  "lint-staged": {
    "*.{js,jsx,mjs,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,less,scss,json,graphql}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "pre-push": "yarn prepush"
    }
  }
}
